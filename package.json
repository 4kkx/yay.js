{
    "name": "yay.js",
    "version": "0.0.4",
    "description": "Simple and intuitive, yet full featured API wrapper for https://yay.space written in TypeScript/JavaScript.",
    "type": "commonjs",
    "source": "./src/index.ts",
    "main": "./build/index.js",
    "types": "./build/index.d.ts",
