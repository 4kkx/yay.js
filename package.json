    "homepage": "https://github.com/qvco/yay.js#readme",
    "files": [
        "build"
    ],
    "dependencies": {
        "axios": "^1.5.0",
        "colors": "^1.4.0",
        "sharp": "^0.32.6",
        "uuid": "^9.0.1",
        "winston": "^3.10.0",
        "ws": "^8.14.2"
    },
    "devDependencies": {
        "@babel/core": "^7.22.20",
        "@babel/preset-env": "^7.22.20",
        "@types/jest": "^29.5.5",
        "@types/node": "^20.6.2",
        "@types/uuid": "^9.0.4",
        "@types/ws": "^8.5.7",
        "babel-jest": "^29.7.0",
        "jest": "^29.7.0",
        "rimraf": "^5.0.1",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "tslib": "^2.6.2",
        "typedoc": "^0.25.2",
